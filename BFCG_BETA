-- Forward-declare cheat frames
local Cheat1_Frame, Cheat2_Frame, Cheat3_Frame

local function updateCheatPages()
	if Page == 1 then
		if Cheat1_Frame then Cheat1_Frame.Visible = true end
		if Cheat2_Frame then Cheat2_Frame.Visible = false end
		if Cheat3_Frame then Cheat3_Frame.Visible = false end
	elseif Page == 2 then
		if Cheat1_Frame then Cheat1_Frame.Visible = false end
		if Cheat2_Frame then Cheat2_Frame.Visible = true end
		if Cheat3_Frame then Cheat3_Frame.Visible = false end
	elseif Page == 3 then
		if Cheat1_Frame then Cheat1_Frame.Visible = false end
		if Cheat2_Frame then Cheat2_Frame.Visible = false end
		if Cheat3_Frame then Cheat3_Frame.Visible = true end
	end
end

